{
    "$schema": "http://schema.management.azure.com/schemas/2015-10-01-preview/policyDefinition.json",
    "if": {
        "allOf": [
            {
                "field": "type",
                "equals": "Microsoft.Network/virtualNetworks"
            },
            {
                "not": {
                    "field": "name",
                    "in": ["internal-vnet", "external-vnet", "management-vnet"]
                }
            }
        ]
    },
    "then": {
        "effect": "deny"
    }
}
